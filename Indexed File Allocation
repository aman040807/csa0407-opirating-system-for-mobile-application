#include <stdio.h>

int main() {
    int index[50], n, i, block;
    printf("Enter index block: ");
    scanf("%d",&block);
    printf("Enter number of blocks: ");
    scanf("%d",&n);

    printf("Enter blocks: ");
    for(i=0;i<n;i++) scanf("%d",&index[i]);

    printf("File indexed at %d -> ", block);
    for(i=0;i<n;i++) printf("%d ", index[i]);
    return 0;
}
