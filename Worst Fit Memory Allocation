#include <stdio.h>
int main(){
    int b[10]={100,500,200,300,600};
    int p[10]={212,417,112,426};
    int nb=5,np=4,alloc[10];
    for(int i=0;i<np;i++) alloc[i]=-1;

    for(int i=0;i<np;i++){
        int worst=-1;
        for(int j=0;j<nb;j++)
            if(b[j]>=p[i])
                if(worst==-1 || b[j]>b[worst]) worst=j;
        if(worst!=-1){ alloc[i]=worst; b[worst]-=p[i]; }
    }
    printf("Process\tSize\tBlock\n");
    for(int i=0;i<np;i++){
        if(alloc[i]!=-1) printf("P%d\t%d\t%d\n",i+1,p[i],alloc[i]+1);
        else printf("P%d\t%d\tNot Allocated\n",i+1,p[i]);
    }
}
