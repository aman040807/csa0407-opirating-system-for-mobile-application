#include <stdio.h>

int main() {
    int b[10], p[10], nb, np, alloc[10];
    printf("Enter number of blocks: ");
    scanf("%d", &nb);
    printf("Enter block sizes:\n");
    for (int i = 0; i < nb; i++)
        scanf("%d", &b[i]);
    printf("Enter number of processes: ");
    scanf("%d", &np);
    printf("Enter process sizes:\n");
    for (int i = 0; i < np; i++)
        scanf("%d", &p[i]);

    for (int i = 0; i < np; i++)
        alloc[i] = -1;

    for (int i = 0; i < np; i++) {
        for (int j = 0; j < nb; j++) {
            if (b[j] >= p[i]) {
                alloc[i] = j;
                b[j] -= p[i];
                break;
            }
        }
    }

    printf("\nProcess\tSize\tBlock\n");
    for (int i = 0; i < np; i++) {
        if (alloc[i] != -1)
            printf("P%d\t%d\tB%d\n", i+1, p[i], alloc[i]+1);
        else
            printf("P%d\t%d\tNot Allocated\n", i+1, p[i]);
    }
    return 0;
}
